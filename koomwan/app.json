{
  "expo": {
    "name": "koomwan",
    "slug": "koomwan",
    "scheme": "koomwan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.koomwan.koomwan"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/K2D-Regular.ttf",
            "./assets/fonts/K2D-Bold.ttf",
            "./assets/fonts/K2D-Medium.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "5b458421-c7e7-4656-b1c9-04dfbfcdcd98"
      }
    }
  }
}
